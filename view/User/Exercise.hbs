  <div class="container mt-4">
    <!-- cards below -->
    <!-- Time Table Section Starts below  -->
    <h1 class="text-center mt-3"
      style="border: 2px solid white; background-color: black; color: red; font-size: 35px; border-radius: 10px;">
      Fitness Plan</h1>
    <div class="container">
      <!-- monday  -->
      <div class="row">
        <h3 class="text-center mt-4 mb-4 p-2 exec-head" style="background: black; color: white;">Monday - (Back)</h3>
        {{#each cards.backdata}}
        <div class="card-group col-lg-4 col-sm-6 mb-3 Home-Cards">
          <div class="card">
            <img src="{{gifUrl}}" class="card-img-top text-center">
            <div class="card-body text-center">
              <h4 class="card-title text-center mb-3" style="color:black;">{{name}}</h4>
              <h5 class="card-title text-center mb-3" style="color:black;">{{equipment}}</h5>
            </div>
            <button class="btn btn-danger exec-btn mt-3 mb-2 done" id="" data-exercise-name="{{name}}">Done</button>
          </div>
        </div>
        {{/each}}
      </div>

      {{!-- Tuesday --}}
      <div class="row">
        <h3 class="text-center mt-4 mb-4 p-2 exec-head" style="background: black; color: white;">Tuesday - (Shoulder)</h3>
        {{#each  cards.shouldersdata}}
